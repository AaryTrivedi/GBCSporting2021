@model Registration
@{
    ViewData["Title"] = "Delete Registration";
}

<form method="post" asp-action="Delete" asp-controller="Registration">
    <h3>Are you sure you want to delete the incident @Model.RegistrationId</h3>
    <input type="hidden" asp-for="RegistrationId" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-route-identifier="@ViewBag.CustomerId" asp-action="Registrations" asp-controller="Registration" class="btn btn-dark">Cancel</a>
</form>